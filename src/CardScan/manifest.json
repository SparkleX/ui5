{
	"_version": "1.12.0",
	"sap.app": {
		"id": "app.logistics.bp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"libs": {
		"sap.ui.layout": {}
	},
	"sap.ui": {
		"technology": "UI5",
		"fullWidth": true,
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "poc.CardScan.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "poc.CardScan.i18n.i18n"
				}
			}
		}
	}
}